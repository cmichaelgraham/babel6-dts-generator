{
  "name": "babel6-dts-generator",
  "version": "0.2.19",
  "description": "Babel6 DTS generator",
  "main": "lib/index.js",
  "scripts": {
    "build": "node node_modules/babel-cli/bin/babel.js src --out-dir lib --source-maps --stage 1",
    "lint": "node node_modules/eslint/bin/eslint.js src/**/*.js",
    "test": "npm run build && node node_modules/babel-cli/bin/babel-node.js lib/test.js",
    "watch": "node node_modules/nodemon/bin/nodemon.js --exec \"npm run test\" --watch src --watch spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cmichaelgraham/babel6-dts-generator.git"
  },
  "keywords": [
    "aurelia",
    "babel"
  ],
  "author": "cmichaelgraham",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/cmichaelgraham/babel6-dts-generator/issues"
  },
  "homepage": "https://github.com/cmichaelgraham/babel6-dts-generator",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cli-color": "^1.1.0",
    "diff": "^2.2.1",
    "eslint": "^1.10.3",
    "glob-promise": "^1.0.4",
    "mimosa": "^2.3.32",
    "mimosa-babel": "^0.5.2",
    "node-inspector": "^0.12.5",
    "nodemon": "^1.8.1"
  }
}
